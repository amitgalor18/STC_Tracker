cd TransCenter_official

TransCenter V2:
MOT17:

private:
python ./tracking/mot17_private_test.py --data_dir=YourPathTo/MOT17/

public:
python ./tracking/mot17_pub_test.py --data_dir=YourPathTo/MOT17/

